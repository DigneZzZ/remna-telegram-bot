# Remnawave Admin Bot Configuration
# Copy this file to .env and fill in your actual values

# Remnawave API Configuration
API_BASE_URL=https://your-remnawave-api.com
REMNAWAVE_API_TOKEN=your_remnawave_api_token_here

# Telegram Bot Configuration  
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrSTUvwxYZ

# Admin Access Control
# Comma-separated list of Telegram user IDs that can access the bot
ADMIN_USER_IDS=123456789,987654321

# Optional: Logging Configuration
LOG_LEVEL=INFO

# Optional: Bot Configuration
BOT_USERNAME=your_bot_username
